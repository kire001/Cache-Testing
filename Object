//Object file
class superHero{
  private var erik = {serial:"Star Trek",
                      hospoda:"U Junaka",
                      barva: "Fialova"};
  private var filip = {serial:"Dota daily",
                      hospoda:"Healing salve",
                      barva: "The same that Purge likes"};

  private var hero;

  public function superHero(userid){
    if (userid=='erik'){
      this.hero=erik;
    }
    else{
    	this.hero=filip;
    }
  }

  public function getSuperHero(){
    return hero;
  }
}
